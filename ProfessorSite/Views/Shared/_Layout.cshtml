                </div>
                <!-- END HEADER -->        
        
                    @RenderBody()
           
               <!-- START TWITTER -->
                <div id="twitter-slider" class="group">
                    <div class="tweets-list"></div>
                    <div class="bird"></div>
                </div>
                <!-- END TWITTER -->   


